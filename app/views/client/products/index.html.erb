<% @products.each do |product| %>
  
  <h2><a href="/client/products/<%= product['id'] %>">Name: <%= product["name"] %></a></h2>
  <h3>Category: <%= product["category"] %></h3>
  <b>Price: <%= product["price"] %><br>
  Description: <%= product["description"] %><br>
  Color: <%= product["color"] %></b>
<% end %>